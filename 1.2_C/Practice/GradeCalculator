#include <stdio.h>

int main(){
    float s1, s2,s3,s4,s5,per;

    printf("Enter your grade in 5 subjects : ");
    scanf("%f %f %f %f %f", &s1, &s2, &s3, &s4, &s5);

    per = (s1+s2+s3+s4+s5)/5.0;

    printf("The percentage is : %f", per);

    if(per>90.0){
        printf("your grade is O");
    }

    else if(per>80.0 ){
        printf("your grade is A+");
    }

    else if(per>70.0 ){
        printf("your grade is A");
    }

    else if(per>60.0 ){
        printf("your grade is B+");
    }

    else if(per>50.0 ){
        printf("your grade is B");
    }

    else if(per<40.0){
        printf("your grade is F");
    }

    return 0;

}